<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.insperion.sets.alarma">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddGroupActivity"
            android:label="@string/label_activity_addgroup"/>
        <activity
            android:name=".GroupsActivity"
            android:label="@string/label_activity_groups"/>
        <activity
            android:name=".ReportBadUseActivity"
            android:label="@string/label_activity_reportbaduse"/>
    </application>

</manifest>